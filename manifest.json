{
  "manifest_version": 2,
  "name": "YouTube Ultimate Comment Enhancer",
  "short_name": "YT Ult Comment Enhancer",
  "version": "1.4.0.5",
  "description": "Subscriber Count \u2022 Location \u2022 Creation Date \u2022 Total Videos \u2022 Latest Video's View Count & Last Uploaded \u2022 Description \u2022 Link",
  "author": "Nh\u00e2n Tr\u00ed",
  "content_scripts": [
    {
      "matches": [
        "https://www.youtube.com/*"
      ],
      "js": [
        "content.js"
      ],
      "all_frames": false,
      "run_at": "document_end"
    }
  ],
  "permissions": [
    "https://www.youtube.com/*"
  ],
  "icons": {
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "64": "icons/icon64.png",
    "128": "icons/icon128.png"
  },
  "web_accessible_resources": [
    "icons/icon32.png",
    "icons/icon48.png",
    "icons/icon64.png",
    "icons/icon128.png"
  ],
  "browser_specific_settings": {
    "gecko": {
      "id": "{a271ed8b-79b5-4c55-88d4-7ddeb399fb26}"
    }
  }
}