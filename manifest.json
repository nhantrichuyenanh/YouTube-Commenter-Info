{
  "manifest_version": 2,
  "name": "YouTube Ultimate Comment Enhancer",
  "short_name": "YT Ult Comment Enhancer",
  "version": "1.3.5",
  "description": "Subscriber Count • Location • Creation Date • Total Videos • Latest Video's View Count & Last Uploaded • Description • Link",
  "author": "Nhân Trí",
  "content_scripts": [
    {
      "matches": [
        "https://www.youtube.com/watch*",
        "https://www.outube.com/shorts/*",
        "https://www.youtube.com/channel/*/community*"
      ],
      "js": [
        "content.js"
      ],
      "all_frames": false,
      "run_at": "document_end"
    }
  ],
  "permissions": [
    "https://www.youtube.com/*"
  ],
  "icons": {
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "64": "icons/icon64.png",
    "128": "icons/icon128.png"
  },
  "web_accessible_resources": [
    "icons/icon32.png",
    "icons/icon48.png",
    "icons/icon64.png",
    "icons/icon128.png"
  ]
}